<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value eval="[('module', '=', 'crm'), ('model', '=', 'crm.stage')]"/>
        </function>
        <value eval="{'noupdate': False}"/>
    </function>
    <record model="crm.stage" id="crm.stage_lead1">
        <field name="name">New</field>
    </record>
    <record model="crm.stage" id="crm.stage_lead2">
        <field name="name">First contact</field>
    </record>
    <record model="crm.stage" id="crm.stage_lead3">
        <field name="name">First meeting</field>
    </record>
    <record model="crm.stage" id="crm.stage_lead4">
        <field name="name">Start coaching</field>
    </record>
    <record model="crm.stage" id="stage_lead5">
        <field name="name">Team meeting</field>
        <field name="sequence">5</field>
    </record>
    <record model="crm.stage" id="stage_lead6">
        <field name="name">Redirection</field>
        <field name="sequence">75</field>
    </record>
    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value eval="[('module', '=', 'crm'), ('model', '=', 'crm.stage')]"/>
        </function>
        <value eval="{'noupdate': True}"/>
    </function>
</odoo>